<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.rfg.tprm.export.bo.Questionare" table="questionare">
      <meta attribute="class-description">
         This class contains the User detail. 
      </meta>
      <meta attribute="sync-DAO">false</meta>
      <id name="id" type="java.lang.Integer" column="q_id">
         <generator class="assigned"/>
      </id>
      <property name="question" column="question" type="string"/>
      <property name="quesType" column="q_type" type="string"/>
      <property name="answer" column="answer" type="string"/>
      <property name="ansOption" column="ans_option" type="string"/>
   </class>
</hibernate-mapping>